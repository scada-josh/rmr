.leaflet-map-pane {
	z-index: 2 !important;
}
.leaflet-google-layer{
	z-index: 1 !important;
}


.info {
  padding: 6px 8px;
  font: 14px/16px Arial, Helvetica, sans-serif;
  background: white;
  background: rgba(255,255,255,0.9);
  box-shadow: 0 0 15px rgba(0,0,0,0.2);
  border-radius: 5px;
  height: auto;
  width: 330px;
  cursor:move;
}
.info h4 {
  margin: 10 0 5px;
  color: #777;
}

.legend {
  /*line-height: 18px;*/
  /*height: auto;*/
}
.legend td {
  opacity: 0.75;
}







