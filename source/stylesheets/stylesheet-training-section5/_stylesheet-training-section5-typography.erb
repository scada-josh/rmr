// Typography

h1, h2, h3, h4, h5, h6{
	margin: 0 0 35px;
	text-transform:uppercase;
	font-family: "Montserrat","Helvetica Neue",Helvetica,Arial,sans-serif;
	letter-spacing:1px;
}

p{
	margin: 0 0 25px;
	font-size:18px;
	line-height:1.6em;
}

a{
	color:$primary;

}

a:hover,a:focus{
	text-decoration:none;
	color: darken($primary, 20%);
}