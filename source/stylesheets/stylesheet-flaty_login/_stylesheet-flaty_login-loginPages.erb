    border-right: 2px solid #ddd;
    border-top-width: 1px;
    margin: 0 -2px 0 -1px;
}
.tabbable .nav-tabs > li > a > .badge {
    line-height: 8px;
    opacity: 0.7;
}
.tabbable .nav-tabs > li > a > [class*="fa-"] {
