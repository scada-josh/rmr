LeafLet: function () {
    console.log('LeafLet');

    L.Icon.Default.imagePath = '../../../../bower_components/leaflet/images/'

    // Add Multiple tile layer
    <%= partial js_dir + "/javascript-unit-test-leaflet/partials/fn_LeafLet/_add_multiple_tile_layer" %>

	map = L.map('map', {
							layers: [googleRoadmapLayer] // only add one! 
						})
			.setView([13.708189, 100.599608], 14);

    // Add Single Marker
    <%= partial js_dir + "/javascript-unit-test-leaflet/partials/fn_LeafLet/_add_single_marker" %>

    // Add Multiple Marker
    <%= partial js_dir + "/javascript-unit-test-leaflet/partials/fn_LeafLet/_add_multiple_marker" %>

    // Add Control
    <%= partial js_dir + "/javascript-unit-test-leaflet/partials/fn_LeafLet/_add_control" %>

    // Script for adding marker on map click
        // function onMapClick(e) {


        // }

        map.on('click', function(e) {
            fn.LeafLet_onMapClick(e)
        });

},
